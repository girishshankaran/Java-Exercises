package example.com;

public class GreatestNumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a=10;
		int b=20;
		
		System.out.println("Large is" + calculate(a, b));
		
		

	}

	private int calculate(int a, int b) {
		// TODO Auto-generated method stub
		int large;
		if (a>b)
			 large=a;
		else
			large=b;
		return (large);
	}

}
